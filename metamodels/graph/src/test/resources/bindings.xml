<?xml version="1.0" encoding="UTF-8"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
  package-name="com.rigiresearch.middleware.graph">
  <java-types>
    <java-type name="Graph">
      <xml-root-element name="elements" />
      <java-attributes>
        <xml-element java-attribute="nodes" name="element" />
      </java-attributes>
    </java-type>
    <java-type name="Node">
      <java-attributes>
        <xml-element java-attribute="metadata" name="option">
          <xml-element-wrapper name="options" />
        </xml-element>
      </java-attributes>
    </java-type>
    <java-type name="Property">
      <java-attributes>
        <xml-attribute java-attribute="name" name="key"/>
      </java-attributes>
    </java-type>
  </java-types>
</xml-bindings>
